{% extends 'layout.main.html.twig' %}


{% block breadcrumbs %}
    {% set breadcrumbs = [
        {'url': path('category.list'), 'title': 'Categories'},
        {'url': path('category.add'), 'title': 'Add', 'active': true},
    ] %}
    {{ parent() }}
{% endblock %}

{% block content %}

    <form action="{{ path('category.add') }}" method="post" novalidate="novalidate">
        {{ form_rest(form) }}
        <button type="submit" class="btn btn-info">Submit</button>
    </form>
{% endblock %}

