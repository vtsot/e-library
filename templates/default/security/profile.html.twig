{% extends 'layout.main.html.twig' %}


{% block breadcrumbs %}
    {% set breadcrumbs = [
        {'url': path('profile'), 'title': 'Profile'},
    ] %}
    {{ parent() }}
{% endblock %}

{% block content %}

    <form action="{{ path('profile') }}" method="post" novalidate="novalidate">
        {{ form_rest(form) }}
        <button type="submit" class="btn btn-info">Submit</button>
    </form>
{% endblock %}

